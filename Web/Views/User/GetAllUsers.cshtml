@model IEnumerable<Application.DTOs.Users.GetUserDTO>

<h1>Users</h1>
<table class="table">
	<thead>
		<tr>
			<th>ID</th>
			<th>UserName</th>
			<th>Email</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var user in Model)
		{
			<tr>
				<td>@user.Id</td>
				<td>@user.UserName</td>
				<td>@user.Email</td>
			</tr>
		}
	</tbody>
